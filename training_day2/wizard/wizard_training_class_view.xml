<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="wizard_training_class_view_form" model="ir.ui.view">
        <field name="name">wizard.training.class.view.form</field>
        <field name="model">wizard.training.class</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="start_date" />
                        <field name="end_date" />
                    </group>
                </sheet>
                <footer>
                    <button string="Change" name="action_change_date"
                        type="object" class="oe_highlight" />
                    <button string="Discard" class="btn btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>